
    {
      "name": "Extension Menu",
      "version": "1.0",
      "manifest_version": 2,
      "description": "My extension",
      "browser_action": {
        "default_icon": "icon.png",
        "default_menu": "Menu",
        "default_popup": "popup.html"
      },
      "icons": {
        "128": "icon.png"
      },
      "permissions": [
          "tabs", "http://localhost:3000/*", "activeTab", "storage"
        ],
			"content_scripts": [
        {
            "matches": [
                "http://localhost:3000/*"
            ],
            "js": [
                "login.js"
            ]
        }
			]
    }